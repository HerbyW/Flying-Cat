<PropertyList>

 <path>instrument_box3.ac</path>

 <offsets>
  <!-- x/y/z == back/right/up -->  <heading-deg>270</heading-deg>
  <x-m> 0.0 </x-m>
  <y-m> 0.0 </y-m>
  <z-m> -0.120 </z-m>
 </offsets>
 
 <animation>
        <type>material</type>
         <object-name>Cube.001</object-name>         
        <emission>
            <factor-prop>controls/switches/gauge-light</factor-prop>
            <red>0.15</red>
            <green>0.15</green>
            <blue>0.15</blue>
        </emission>        
    </animation>
 

